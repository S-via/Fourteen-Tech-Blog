<form id="#signupform">
    <h1>Sign Up</h1>
  <div class="mb-3">
    <label for="usernamesu" class="form-label">Username</label>
    <input type="username" class="form-control" id="usernameinput" aria-describedby="emailHelp">
    <div id="userHelp" class="form-text">Create A New Username </div>
  </div>
  <div class="mb-3">
    <label for="passwordsu" class="form-label">Password</label>
    <input type="password" class="form-control" id="passwordinput">
  </div>
  <button type="submit" class="btn btn-primary">Submit</button>
  <!-- add link to login-->
  <div><a href ="/">Log In</a></div>
</form>

